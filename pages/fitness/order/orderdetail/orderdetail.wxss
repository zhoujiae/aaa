.predetail {
  width: 95%;
  margin: 30rpx auto;
}
.pre_input {
  width: 100%;
}
input {
  width: 100%;
  height: 100rpx;
  background: white;
  border-radius: 10rpx;
}
button {
  background: #ffd101;
  color: white;
  width: 95%;
  margin: 10rpx auto;
}
checkbox-group {
  display: flex;
  justify-content: space-around;
  flex-wrap: wrap;
  flex-direction: row;
}
label {
  margin: 10rpx 0;
}
picker {
  background: white;
  padding: 20rpx 0;
  border-radius: 10rpx;
  color: black;
}
page {
  background: cornsilk;
}
textarea {
  background: white;
  width: 100%;
}